        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.md5.template2.Activty_login"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.md5.template2.Activity_passcreate"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.md5.template2.Activity_regi"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.md5.template2.Activiry_setting"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.md5.template2.Activity_profile"
            android:screenOrientation="portrait" />
    </application>

</manifest>